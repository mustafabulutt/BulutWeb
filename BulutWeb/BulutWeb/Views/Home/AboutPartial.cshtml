@model ICollection<BulutWeb.Models.Model.About>
@{
    Layout = null;
}
<div id="myCarousel" class="carousel slide">
    <div class="carousel-inner">

        @foreach (var item in Model)
        {
            <div class="item active">
                <img src="@item.Image" class="img-responsive" style="width:580px;height:440px;" alt="" />
                <div class="carousel-caption success">

                    <p>@item.ImageInformation</p>
                </div>
            </div>
        }


    </div>

</div>
            </div>

<div class="span6">

    @foreach (var item in Model)
    {
        <h4>@item.Title</h4>

        <blockquote>
            @Html.Raw(item.Description)
        </blockquote>
    }

